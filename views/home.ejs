    <section class="feedback" id="feedback">
        <h3>Feedback</h3>
        <form action="/students_feedback" method="post" class="formEntry">
            <div class="row">
                <div class="input-field col m12 s12 l12">
                    <input type="text" name="username" id="myname" class="validate" />
                    <label for="myname">Student Name</label>
                </div>
                <div class="input-field col m12 s12 l12">
                    <select name="year">
                        <option value="" disabled selected>Choose...</option>
                        <option value="Second Year">Second Year</option>
                        <option value="Third Year">Third Year</option>
                        <option value="Fourth Year">Fourth Year</option>
                    </select>
                    <label>Year</label>
                </div>
                <div class="input-field col m12 s12 l12">
                    <select name="branch">
                        <option value="" disabled selected>Choose...</option>
                        <option value="CSE">CSE</option>
                        <option value="ECE">ECE</option>
                        <option value="EEE">EEE</option>
                        <option value="MECH">MECH</option>
                    </select>
                    <label>Branch</label>
                </div>
                <div class="input-field col m12 s12 l12">
                    <input type="text" name="feed_title" id="myfeed" class="validate" />
                    <label for="myfeed">Title</label>
                </div>
                <div class="input-field col m12 s12 l12">
                    <input type="text" name="desc" id="mydesc" class="validate" />
                    <label for="mydesc">Feedback</label>
                </div>
            </div>
            <button type="submit" class="waves-effect waves-light btn-large send">
                Submit</button><br /><br />
        </form>
    </section>
